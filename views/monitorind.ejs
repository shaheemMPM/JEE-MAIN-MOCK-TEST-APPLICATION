<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <title>Create Students | Dashboard | INSIGHT PLUS | JEE MAINS - Mock Test</title>
    <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0, shrink-to-fit=no' name='viewport' />
    <!-- CSS Files -->
    <link href="../../css/bootstrap.min.css" rel="stylesheet" />
    <link href="../../css/light-bootstrap-dashboard.css" rel="stylesheet" />
    <style media="screen">
      .cs-tbl{
        background-color: white;
      }
      th{
        font-size: 16px !important;
        font-weight: bolder !important;
        padding-left: 15px !important;
      }
      td{
        padding-left: 15px !important;
      }
      .trash{
        width: 25px;
        height: 25px;
      }
      .td-img{
        width: 100px;
      }
    </style>
  </head>
  <body>
    <div class="wrapper">
        <div class="sidebar" data-image="images/sidebar-5.jpg">
            <div class="sidebar-wrapper">
                <div class="logo">
                    <a href="#" class="simple-text">
                        INSIGHT PLUS
                    </a>
                </div>
                <ul class="nav">
                    <li>
                        <a class="nav-link" href="/dashboard">
                            <i class="nc-icon nc-chart-pie-35"></i>
                            <p>Dashboard</p>
                        </a>
                    </li>
                    <li>
                        <a class="nav-link" href="/dashboard/uploadqn">
                            <i class="nc-icon nc-cloud-upload-94"></i>
                            <p>Upload Questions</p>
                        </a>
                    </li>
                    <li>
                        <a class="nav-link" href="/dashboard/viewqn">
                            <i class="nc-icon nc-light-3"></i>
                            <p>View Questions</p>
                        </a>
                    </li>
                    <li>
                        <a class="nav-link" href="/student/createstd">
                            <i class="nc-icon nc-circle-09"></i>
                            <p>Create Students</p>
                        </a>
                    </li>
                    <!-- <li>
                        <a class="nav-link" href="/createad">
                            <i class="nc-icon nc-notification-70"></i>
                            <p>Create Test</p>
                        </a>
                    </li>
                    <li>
                        <a class="nav-link" href="/admoniter">
                            <i class="nc-icon nc-notes"></i>
                            <p>Monitor Test</p>
                        </a>
                    </li> -->
                    <li class="nav-item active">
                        <a class="nav-link" href="/student/moniter">
                            <i class="nc-icon nc-notes"></i>
                            <p>Monitor Students</p>
                        </a>
                    </li>
                    <!-- <li>
                        <a class="nav-link" href="/verifysubmissions">
                            <i class="nc-icon nc-badge"></i>
                            <p>View Submissions</p>
                        </a>
                    </li> -->
                </ul>
            </div>
        </div>
        <div class="main-panel">
            <!-- Navbar -->
            <nav class="navbar navbar-expand-lg " color-on-scroll="500">
                <div class="container-fluid">
                    <a class="navbar-brand" href="#pablo"> Create Students </a>
                    <div class="collapse navbar-collapse justify-content-end" id="navigation">
                        <ul class="navbar-nav ml-auto">
                            <li class="nav-item">
                                <a class="nav-link" href="/logout">
                                    <span class="no-icon">Log out</span>
                                </a>
                            </li>
                        </ul>
                    </div>
                </div>
            </nav>
            <!-- End Navbar -->
            <div class="content">
              <div class="container-fluid">
                <div class="row">
                  <div class="col-12">
                    <table class="table table-bordered table-hover cs-tbl">
                      <thead>
                        <tr>
                          <th>Register Number</th>
                          <th>Full Name</th>
                          <th>DOB</th>
                          <th>Marks</th>
                        </tr>
                      </thead>
                      <tbody>
                        <tr>
                          <td><%= data.regno %></td>
                          <td><%= data.name %></td>
                          <td><%= data.dob %></td>
                          <td><%= data.mtotal %></td>
                        </tr>
                      </tbody>
                    </table>
                  </div>
                  <div class="col-12">
                    <table class="table table-bordered table-hover cs-tbl">
                      <thead>
                        <tr>
                          <th>Exam Status</th>
                          <th>Exam Started Time</th>
                          <th>Exam Finished Time</th>
                        </tr>
                      </thead>
                      <tbody>
                        <tr>
                          <td>
                            <% if(data.exam_stat == 0){ %>
                              <span style="color: rgb(233, 47, 0)">Not Started</span>
                            <% }else if(data.exam_stat == 1){%>
                              <span style="color: rgb(0, 108, 255)">Started</span>
                            <% }else{ %>
                              <span style="color: rgb(66, 150, 77)">Finished</span>
                            <% } %>
                          </td>
                          <td><%= data.start_time %></td>
                          <td><%= data.end_time %></td>
                        </tr>
                      </tbody>
                    </table>
                  </div>
                  <div class="col-12">
                    <table class="table table-bordered table-hover cs-tbl">
                      <thead>
                        <tr>
                          <th>Qno</th>
                          <th>Status</th>
                          <th>Answer Given</th>
                          <th>Mark</th>
                        </tr>
                      </thead>
                      <tbody>
                        <% for(var i = 1; i <= 75; i++) { %>
                          <tr>
                            <td><%= i %></td>
                            <td>
                              <% if(data.qstatus[i] == 0){ %>
                                <span style="color: rgb(0, 108, 255)">Not Visited</span>
                              <% }else if(data.qstatus[i] == 1){%>
                                <span style="color: rgb(233, 47, 0)">Not Answered</span>
                              <% }else if(data.qstatus[i] == 2){%>
                                <span style="color: rgb(153, 66, 210)">Marked</span>
                              <% }else{ %>
                                <span style="color: rgb(66, 150, 77)">Answered</span>
                              <% } %>
                            </td>
                            <td><%= data.ans[i] %></td>
                            <td><%= data.marks[i] %></td>
                          </tr>
                        <% } %>
                      </tbody>
                    </table>
                  </div>
                  <div class="col-12">
                    <table class="table table-bordered table-hover cs-tbl">
                      <thead>
                        <tr>
                          <th align="center">Student Activities Log</th>
                        </tr>
                      </thead>
                      <tbody>
                        <% for(var i = 0; i < data.activities.length; i++){ %>
                          <tr>
                            <td><%= data.activities[i] %></td>
                          </tr>
                        <% } %>
                      </tbody>
                    </table>
                  </div>
                </div>
              </div>
            </div>
            <footer class="footer">
              <div class="container-fluid">
                <nav>
                  <ul class="footer-menu">
                    <li>
                      <a href="#" id="btnExport">Export All</a>
                    </li>
                  </ul>
                </nav>
              </div>
            </footer>
        </div>
    </div>
    <!--   Core JS Files   -->
    <script src="../../js/jquery.3.2.1.min.js" type="text/javascript"></script>
  </body>
</html>
